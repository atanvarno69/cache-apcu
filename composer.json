{
    "name": "atanvarno/cache-apcu",
    "description": "APCu cache driver",
    "keywords": [
         "APCu", "cache"
    ],
    "homepage": "https://github.com/atanvarno69/cache",
    "license": "MIT",
    "authors": [
        {
            "name": "atanvarno69",
            "email": "atanvarno69@gmail.com",
            "homepage": "https://github.com/atanvarno69",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-apcu": "*",
        "atanvarno/cache": "^0.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.8"
    },
    "autoload": {
        "psr-4": {
